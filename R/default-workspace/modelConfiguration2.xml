    <SpeciesConfigurationFile>species-configuration.xml</SpeciesConfigurationFile>
    <RandomStateFile>randomstate.xml</RandomStateFile>
    <InputPath/>
  </InputConfiguration>
  <OutputConfiguration>
    <OutputDirectory>output</OutputDirectory>
    <CsvFieldDelimiter>,</CsvFieldDelimiter>
    <Aggregations>
      <OutputAggregation>
        <Name>elevation</Name>
        <Map>Dem.txt</Map>
      </OutputAggregation>
      <OutputAggregation>
        <Name>landtype</Name>
        <Map>Landtype.txt</Map>
      </OutputAggregation>
    </Aggregations>
