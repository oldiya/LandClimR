  </OutputConfiguration>
  <ClimateConfiguration>
    <ClimateFile>Dischma_Climate_random.dat</ClimateFile>
    <Format>Yearly</Format>
    <Randomization>Restart</Randomization>
    <WaterBalanceModel>ORIGINAL</WaterBalanceModel>
  </ClimateConfiguration>
  <PlantingDisturbanceConfiguration>
    <Enabled>false</Enabled>
    <ParameterFile>planting-parameters.xml</ParameterFile>
  </PlantingDisturbanceConfiguration>
  <FireDisturbanceConfiguration>
    <Enabled>false</Enabled>
    <ProbExp>2.5</ProbExp>
    <ProbExpBeta>1</ProbExpBeta>
    <ProbPMax>1</ProbPMax>
    <ProbPMin>0</ProbPMin>
    <SizeMax>20</SizeMax>
    <SpreadFunction>IncompleteBetaDistribution</SpreadFunction>
    <DroughtIndex>Default</DroughtIndex>
  </FireDisturbanceConfiguration>
  <WindDisturbanceConfiguration>
    <Enabled>false</Enabled>
    <WindthrowModel>DEFAULT</WindthrowModel>
    <BreakoutCoefficient>0.002</BreakoutCoefficient>
    <MeanWindthrowSize>1250</MeanWindthrowSize>
    <ProbabilityCoefficient>200</ProbabilityCoefficient>
    <SizeCoefficient>0.2</SizeCoefficient>
    <MeanReturnInterval>800</MeanReturnInterval>
    <MinWindthrowSize>620</MinWindthrowSize>
    <MaxWindthrowSize>2500</MaxWindthrowSize>
  </WindDisturbanceConfiguration>
  <HarvestDisturbanceConfiguration>
    <Enabled>false</Enabled>
    <ParameterFile>harvest-parameters.xml</ParameterFile>
  </HarvestDisturbanceConfiguration>
  <BeetleDisturbanceConfiguration>
    <Enabled>true</Enabled>
    <ParameterFile>barkbeetle-parameters.xml</ParameterFile>
  </BeetleDisturbanceConfiguration>
  <RuntimeConfiguration>
    <StartYear>1</StartYear>
    <EndYear>110</EndYear>
    <RandomInit>
      <X>1</X>
      <Y>10000</Y>
      <Z>3000</Z>
    </RandomInit>
  </RuntimeConfiguration>
  <DispersalConfiguration>
    <DispersalMode>RANDOM_ASYMMETRIC</DispersalMode>
    <BaseSeedProbability>0.001</BaseSeedProbability>
    <EffectiveDm1Probability>0.1</EffectiveDm1Probability>
    <MaxDm1Probability>0.01</MaxDm1Probability>
  </DispersalConfiguration>
  <DebugConfiguration>
    <WaitForExit>true</WaitForExit>
    <DebugOutput>false</DebugOutput>
  </DebugConfiguration>
</ControlFile>
